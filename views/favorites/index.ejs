<% favorites.forEach(function(favorite) { %>
  <div class="well">
    <h2><%= favorite.beerName %>   
    </h2>
    <a href="/beers/show<%= favorite.beerId %>" class="btn btn-primary">Show More Details</a>

    <a href="/favorites/<%= favorite.beerId %>" class="btn btn-danger pull-right delete-btn">Remove</a>
  </div>
<% }); %>